{{!
    @template block_course_audit/structure

    Displays the admin tree as a list of grouped links.

    Example context (json):
    {
        "node": {
            "headline": "Headline"
            "disclaimertext": "Root of menu",
            "buttonText": "test0",
        }
    }
}}
<div id="section">
<div class="container">
    <div class="disclaimer-box">
        <h2>{{headline}}</h2>
        <p>{{disclaimerText}}</p>
    </div>

    <div class="hinweise-box">
        <ul>
            {{#hinweise}}
                <li>{{.}}</li>
            {{/hinweise}}
        </ul>
    </div>
</div>
    <button>{{buttonText}}</button>
</div>
